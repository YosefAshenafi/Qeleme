{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "releaseChannel": "production",
      "distribution": "store",
      "autoIncrement": true,
      "node": "22.11.0",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true",
        "EXPO_NO_TYPESCRIPT_CHECK": "1",
        "SKIP_TYPESCRIPT_CHECK": "1"
      },
      "ios": {
        "cache": {
          "paths": ["node_modules"]
        },
        "buildConfiguration": "Release"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "bundleIdentifier": "com.qelemapp.qelem"
      }
    }
  }
}
