{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "releaseChannel": "production",
      "distribution": "store",
      "autoIncrement": true,
      "node": "22.11.0",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "ios": {
        "cache": {
          "paths": ["node_modules"]
        }
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "bundleIdentifier": "com.yosefashenafi.Qelem"
      }
    }
  }
}
